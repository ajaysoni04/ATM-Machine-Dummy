Private Sub Image1_Click()
Form1.Visible = False
Form2.Visible = True
End Sub

Private Sub Image2_Click()
Form1.Visible = False
Form2.Visible = True
End Sub

Private Sub Timer1_Timer()
If Image1.Visible = True Then
Image1.Visible = False
Image2.Visible = True
ElseIf Image2.Visible = True Then
Image2.Visible = False
Image1.Visible = True
End If
End Sub


Private Sub Image1_Click()
Form3.Visible = True
